<Window x:Class="Powercards.WinPresent.ServerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ServerWindow" Height="300" Width="389" Loaded="Window_Loaded">
    <StackPanel>
        <Button Content="Start Game" Name="btnStartGame" Click="btnStartGame_Click" />
        <TextBox Height="233" HorizontalAlignment="Left" IsReadOnly="True" Name="txtLog" TextWrapping="Wrap" VerticalAlignment="Top" VerticalScrollBarVisibility="Auto" Width="364" />
    </StackPanel>
</Window>
